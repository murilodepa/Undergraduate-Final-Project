{
  "expo": {
    "name": "tabletFrontend",
    "slug": "tabletFrontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/clothing-store.png",
    "splash": {
      "image": "./src/assets/clothing-store.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "widht": 50
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/clothing-store.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
